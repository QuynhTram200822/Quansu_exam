    @await Html.PartialAsync("_Footer")
    <script type="text/javascript" src="./client-app/assets/js/jquery-3.7.1.min.js"></script>
    <script type="text/javascript" src="./client-app/assets/bootstrap-5.0.2-dist/js/bootstrap.bundle.min.js"></script>
    <script type="text/javascript" src="./client-app/assets/slick/slick.min.js"></script>
    <script type="text/javascript" src="./client-app/assets/js/slider.js"></script>
    <script type="text/javascript" src="./client-app/assets/js/script.js"></script>
    <script type="text/javascript" src="./client-app/assets/js/tabs.js" defer></script>
    <script type="text/javascript" src="./client-app/assets/js/getlink.js"></script>
</body>
</html>