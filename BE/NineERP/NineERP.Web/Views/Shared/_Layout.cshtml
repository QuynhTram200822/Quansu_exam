<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <link rel="stylesheet" href="./client-app/assets/bootstrap-5.0.2-dist/css/bootstrap.min.css">
    <!-- Slick CSS -->
    <link rel="stylesheet" type="text/css" href="./client-app/assets/slick/slick.css" />
    <link rel="stylesheet" type="text/css" href="./client-app/assets/slick/slick-theme.css" />
    <link rel="stylesheet" href="./client-app/assets/css/style.css">
    <!-- icon -->
    <link rel="stylesheet" href="./client-app/assets/bootstrap-icons-1.11.3/bootstrap-icons-1.11.3/font/bootstrap-icons.css">
    <title>@ViewData["Title"]</title>
</head>
<body>
    @await Html.PartialAsync("_Header")
